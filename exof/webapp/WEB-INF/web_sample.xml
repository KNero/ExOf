<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	metadata-complete="true" version="3.0">
	<display-name>Micro PushServer</display-name>

	<listener>
		<listener-class>team.balam.exof.web.WebListener</listener-class>
	</listener>
	
	<context-param>
		<param-name>exof.configPath</param-name>
		<param-value>D:/mywork/D.PushServer/WebContent/env</param-value>
	</context-param>
	
	<servlet>
		<servlet-name>exof.webServlet</servlet-name>
		<servlet-class>team.balam.exof.container.console.WebServlet</servlet-class>
		<init-param>
			<param-name>httpServletTransform</param-name>
			<param-value>team.balam.exof.container.HttpServletTransform</param-value>
		</init-param>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>exof.webServlet</servlet-name>
		<url-pattern>/exof/*</url-pattern>
	</servlet-mapping>
</web-app>